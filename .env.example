# Dev local can leave this unset because Vite already proxies /api para 127.0.0.1:8787.
# Em producao, configure a URL publica do seu Worker/API.
VITE_API_BASE_URL=
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_key
VITE_ADMIN_ACCESS_CODE=change_this_admin_code

DATABASE_URL=postgresql://user:password@host/dbname?sslmode=require
CACHE_TTL_SECONDS=600
RATE_LIMIT_PER_MINUTE=120
